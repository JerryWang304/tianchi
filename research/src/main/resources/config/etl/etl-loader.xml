<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="simpleLoader" class="etl.loader.impl.SimpleLoader">
        <property name="extractor" ref="simpleExtractor"/>
        <property name="transferorList" ref="transferorList"/>
        <property name="filePathList">
            <list>
                <value>${data.file1.path}</value>
                <value>${data.file2.path}</value>
            </list>
        </property>
    </bean>


</beans>